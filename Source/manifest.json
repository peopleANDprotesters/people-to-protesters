{
	"manifest_version": 2,
	"name": "Protesters To People",
	"version": "1.0",
	"description": "Replaces the text 'protester' with 'person'.",
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

